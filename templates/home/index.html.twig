{% extends 'base.html.twig' %}

{% block title %}Hello HomeController!{% endblock %}

	{% block body %}

		<div class="container mt-3">


			<div class="jumbotron">
				<h1 class="display-4">Bienvenue sur notre site d'annonces.</h1>
				<p class="lead">Trouvez votre hébergement en quelques clics.</p>
				<hr class="my-4">
				<p>Peu importe votre destination, vous vous sentirez comme à la maison.</p>
				<a class="btn btn-primary btn-lg" href="#" role="button">En savoir plus</a>
			</div>

			<div>

				<h2> Nos appartements </h2>

				

			</div>



			

			<div class='row'> 

				{% for compteur in 1..9 %}

					<div class='col-md-4'>
						<div class="card-deck mb-4">
							<div class="card">
								<div id="carouselExampleIndicators{{ compteur }}" class="carousel slide" data-interval='false' >
									<ol class="carousel-indicators">
										<li data-target="#carouselExampleIndicators{{ compteur }}" data-slide-to="0" class="active"></li>
										<li data-target="#carouselExampleIndicators{{ compteur }}" data-slide-to="1"></li>
										<li data-target="#carouselExampleIndicators{{ compteur }}" data-slide-to="2"></li>
									</ol>
									<div class="carousel-inner">
										<div class="carousel-item active">
											<img src="/images/apt1.jpg" class="d-block w-100" alt="...">
										</div>
										<div class="carousel-item">
											<img src="/images/apt2.jpg" class="d-block w-100" alt="...">
										</div>
										<div class="carousel-item">
											<img src="/images/apt3.jpg" class="d-block w-100" alt="...">
										</div>
									</div>
									<a class="carousel-control-prev" href="#carouselExampleIndicators{{ compteur }}" role="button" data-slide="prev">
										<span class="carousel-control-prev-icon" aria-hidden="true"></span>
										<span class="sr-only">Previous</span>
									</a>
									<a class="carousel-control-next" href="#carouselExampleIndicators{{ compteur }}" role="button" data-slide="next">
										<span class="carousel-control-next-icon" aria-hidden="true"></span>
										<span class="sr-only">Next</span>
									</a>
								</div>
								<div class="card-body">
									<h5 class="card-title">Annonce {{ compteur }} </h5>
									<p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
								</div>
								<a class="btn btn-primary btn-lg" href="#" role="button">En savoir plus</a>
								<div class="card-footer">
									<small class="text-muted">Last updated 3 mins ago</small>
								</div>
							</div>
						</div>
					</div>

				{% endfor %}

			</div>



			

		</div>
	{% endblock %} 



